<h3>Giving a Compliment</h3>
<%= form_for(@prefill) do |f| %>

  <label>Give a Compliment</label><br>
  <br>
  <%=f.collection_select :selection, Prefill.all, :selection,:selection%><br> 
  <%=f.collection_select :user_id, User.all, :id,:name %>
  <%= f.submit %>
